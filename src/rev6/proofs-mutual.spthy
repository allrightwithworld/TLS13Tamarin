                      h(<$C, ~nc, ~sid, $pc, X, $S, ~ns, ~sid, $ps, 'g'^~y, $encext, $S, 
                         pk(~ltkS), $certreq, 
                         sign(h(<'server_certificate_verify', $C, ~nc, ~sid, $pc, X, $S, 
                                 ~ns, ~sid, $ps, 'g'^~y, $encext, $S, pk(~ltkS), $certreq>),
                              ~ltkS)
                        >)
                     >)
                >,
                PRFsecond48(<
                             PRF(<z, 'handshake_master_secret', 
