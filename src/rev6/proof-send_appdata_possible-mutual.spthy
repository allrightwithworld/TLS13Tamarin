   [
   !Ltk( $A, ~ltkA ), !Pk( $A, pk(~ltkA) ),
   AuthenticMessage( pk(~ltkA) )
   ]

  /* has exactly the trivial AC variant */

rule (modulo E) Reveal_ltk:
   [ !Ltk( $A, ~ltkA ) ]
  --[ RevLtk( $A ) ]->
   [ AuthenticMessage( ~ltkA ) ]

  /* has exactly the trivial AC variant */

rule (modulo E) Reveal_fresh:
   [ Fresh( ~tid, ~x ) ]
  --[ RevFresh( ~tid ) ]->
   [ AuthenticMessage( ~x ) ]

  /* has exactly the trivial AC variant */

